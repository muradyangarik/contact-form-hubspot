{
  "name": "contact-form-hubspot",
  "version": "1.0.0",
  "description": "WordPress Contact Form Plugin with HubSpot Integration",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:css",
    "build:js": "webpack --mode=production",
    "build:css": "sass assets/css/frontend.scss assets/css/frontend.css --style=compressed",
    "dev": "npm run dev:js && npm run dev:css",
    "dev:js": "webpack --mode=development --watch",
    "dev:css": "sass assets/css/frontend.scss assets/css/frontend.css --watch",
    "lint": "eslint assets/js/**/*.js",
    "lint:fix": "eslint assets/js/**/*.js --fix"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "contact-form",
    "hubspot",
    "gutenberg"
  ],
  "author": "Your Name",
  "license": "GPL-2.0-or-later",
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "eslint": "^8.42.0",
    "mini-css-extract-plugin": "^2.7.0",
    "sass": "^1.62.0",
    "sass-loader": "^13.3.0",
    "style-loader": "^3.3.0",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  },
  "dependencies": {
    "@wordpress/blocks": "^12.0.0",
    "@wordpress/components": "^25.0.0",
    "@wordpress/element": "^5.0.0",
    "@wordpress/i18n": "^4.0.0"
  }
}



